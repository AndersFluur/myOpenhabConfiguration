// Named persistence strategies for use in the "Items" section
Strategies {
	// cron strategies
	everyMinute : "0 * * * * ?"
	everyHour 	: "0 0 * * * ?"
	everyDay 	: "0 0 0 * * ?"

	// if no strategy is specified for an item entry below, the default list will be used
	default = everyChange
}

Items {
	// Store all sensor values from Tellstick
	td_sensor_mandolyn_11_1, td_sensor_mandolyn_11_2 : strategy = everyMinute
}
